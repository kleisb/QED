<table class="table table-striped table-bordered table-hover table-condensed">
    <thead>
    <tr>
        {{#each headers}}
        <th>{{label}}</th>
        {{/each}}
        {{#if showAdmix}}
        <th><a href="#" class="toggle-admix" onclick="return false;">Admix Ratios</a></th>
        {{/if}}
    </tr>
    </thead>
    <tbody>
    {{#each rows}}
    <tr>
        {{#each values}}
        <td>{{.}}</td>
        {{/each}}
        {{#if showAdmix}}
        <td class="c-admix-ratio"></td>
        {{/if}}
    </tr>
    {{/each}}
    </tbody>
</table>